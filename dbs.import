    IF  P_PROC_ID = 'GET_DPT_LIST' THEN  
      SET @EXEC_ST  = P_START_NO;
      SET @EXEC_CNT  = P_PAGE_NO;            
      SET @ROWNUM =P_START_NO;

      SELECT  COUNT(DPT_NO)
        INTO  P_DPT_CNT
