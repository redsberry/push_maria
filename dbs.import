        INSERT INTO SN_RIGHT (USR_ID, RIG_GRP_CD, USE_YN, UPR_ID, REG_TS, UPD_TS )
             VALUES (P_USR_ID, P_RIGHT_ID, P_USE_YN,P_UPR_ID, NOW(), NOW() )
