CREATE DEFINER=`redspush`@`%` PROCEDURE `GET_PIECHART_REQTIME`(
    IN `P_FROM_DT`    VARCHAR(8),
    IN `P_TO_DT`      VARCHAR(8),
    OUT P_RESULT_ID     INT,
    OUT P_MESSAGE       TINYTEXT
)
    COMMENT '[oslee] 발송요청 시간대별 통계 조회(%)'
BEGIN

    DECLARE SQLSTRING   TEXT;
    DECLARE CNTSTRING   TEXT;
    DECLARE O_COUNT      INT;
        
    SET SQLSTRING = "";  
    SET CNTSTRING = "";
    SET O_COUNT = 0;
    
    SET P_RESULT_ID= 0;
    SET P_MESSAGE= "정상적으로 처리되었습니다.";
    
    SET @EXEC_FROM = P_FROM_DT;
    SET @EXEC_TO = P_TO_DT;

    SET SQLSTRING = CONCAT(SQLSTRING, "SELECT ROUND(((SUM(T.0H)/SUM(T.TOT)) * 100),2) AS '0시', \n");
    SET SQLSTRING = CONCAT(SQLSTRING, "       ROUND(((SUM(T.1H)/SUM(T.TOT)) * 100),2) AS '1시', \n");
    SET SQLSTRING = CONCAT(SQLSTRING, "       ROUND(((SUM(T.2H)/SUM(T.TOT)) * 100),2) AS '2시', \n");
    SET SQLSTRING = CONCAT(SQLSTRING, "       ROUND(((SUM(T.3H)/SUM(T.TOT)) * 100),2) AS '3시', \n");
    SET SQLSTRING = CONCAT(SQLSTRING, "       ROUND(((SUM(T.4H)/SUM(T.TOT)) * 100),2) AS '4시', \n");
    SET SQLSTRING = CONCAT(SQLSTRING, "       ROUND(((SUM(T.5H)/SUM(T.TOT)) * 100),2) AS '5시', \n");
    SET SQLSTRING = CONCAT(SQLSTRING, "       ROUND(((SUM(T.6H)/SUM(T.TOT)) * 100),2) AS '6시', \n");
    SET SQLSTRING = CONCAT(SQLSTRING, "       ROUND(((SUM(T.7H)/SUM(T.TOT)) * 100),2) AS '7시', \n");
    SET SQLSTRING = CONCAT(SQLSTRING, "       ROUND(((SUM(T.8H)/SUM(T.TOT)) * 100),2) AS '8시', \n");
    SET SQLSTRING = CONCAT(SQLSTRING, "       ROUND(((SUM(T.9H)/SUM(T.TOT)) * 100),2) AS '9시', \n");
    SET SQLSTRING = CONCAT(SQLSTRING, "       ROUND(((SUM(T.10H)/SUM(T.TOT)) * 100),2) AS '10시', \n");
    SET SQLSTRING = CONCAT(SQLSTRING, "       ROUND(((SUM(T.11H)/SUM(T.TOT)) * 100),2) AS '11시', \n");
    SET SQLSTRING = CONCAT(SQLSTRING, "       ROUND(((SUM(T.12H)/SUM(T.TOT)) * 100),2) AS '12시', \n");
