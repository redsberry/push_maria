CREATE DEFINER=`redspush`@`%` PROCEDURE `GET_CPN_INFO`(
    IN `P_CPNY_NO`         				  INT(10),
    OUT `P_CPNY_NM`         				VARCHAR(20),
    OUT `P_CPNY_LOG_IMG_PATH`       VARCHAR(50),
    OUT `P_CPNY_ADDR`         		  VARCHAR(50),
    OUT `P_CEO_NM`         		      VARCHAR(20),
