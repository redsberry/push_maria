CREATE DEFINER=`redspush`@`%` PROCEDURE `MSET_MEMBER_INFO_UPDATE`(   P_PROC_ID			    VARCHAR(50)	,
                                            P_USR_ID			    VARCHAR(50)	,
    								       	P_VALUE			        VARCHAR(100),
    								        OUT 	P_RESULT_ID     INTEGER     ,
    									    OUT 	P_MESSAGE	    VARCHAR(500)
								       	)
BEGIN
    SET P_RESULT_ID	= 0;
    SET P_MESSAGE	= "정상적으로 처리되었습니다.";
	
    IF P_PROC_ID = "SET_MEMBER_NICNAME" THEN
		
        IF EXISTS ( SELECT  USR_ID
				    FROM    SN_USR
				    WHERE   USR_ID	= P_USR_ID) THEN
            UPDATE  SN_USR
		    SET     USR_NCKNM	= P_VALUE
		    WHERE   USR_ID	    = P_USR_ID;
        ELSE
		    SET P_RESULT_ID	= -1;
		    SET P_MESSAGE	= "회원아이디가 없습니다.";
        END IF;
	
    ELSEIF P_PROC_ID = "SET_MEMBER_PASSWORD" THEN
		
        IF EXISTS ( SELECT  USR_ID
				    FROM    SN_USR
				    WHERE   USR_ID	= P_USR_ID) THEN
		    UPDATE  SN_USR
		    SET     USR_PW	    = P_VALUE
		    WHERE   USR_ID	    = P_USR_ID;
        ELSE
			SET	P_RESULT_ID	= -1;
			SET	P_MESSAGE	= "회원아이디가 없습니다.";
        END IF;
	
    ELSEIF P_PROC_ID = "SET_MEMBER_MOBILE" THEN
		
        IF EXISTS ( SELECT  USR_ID
				    FROM    SN_USR
				    WHERE   USR_ID	= P_USR_ID) THEN
		    UPDATE  SN_USR
		    SET     CELL_NO	= P_VALUE
		    WHERE   USR_ID	= P_USR_ID;
        ELSE
		    SET P_RESULT_ID	= -1;
		    SET P_MESSAGE	= "회원아이디가 없습니다.";
        END IF;
	
    ELSEIF P_PROC_ID = "SET_MEMBER_PHONE" THEN
		
        IF EXISTS ( SELECT  USR_ID
				    FROM    SN_USR
				    WHERE   USR_ID	= P_USR_ID) THEN
		    UPDATE  SN_USR
		    SET     TEL_NO	= P_VALUE
		    WHERE   USR_ID	= P_USR_ID;
	    ELSE
		    SET P_RESULT_ID	= -1;
		    SET P_MESSAGE	= "회원아이디가 없습니다.";
	    END IF;
	
    ELSEIF P_PROC_ID = "SET_MEMBER_EMAIL" THEN
		
	    IF EXISTS ( SELECT  USR_ID
				    FROM    SN_USR
				    WHERE   USR_ID	= P_USR_ID) THEN
		    UPDATE  SN_USR
		    SET     EMAIL_ADDR	= P_VALUE
		    WHERE   USR_ID	    = P_USR_ID;
	    ELSE
		    SET P_RESULT_ID	= -1;
		    SET P_MESSAGE	= "회원아이디가 없습니다.";
	    END IF;
		
    END IF;

END ;;
