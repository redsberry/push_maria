CREATE DEFINER=`redspush`@`%` PROCEDURE `GET_RIGHT_LIST`(
	IN `P_PROC_ID` VARCHAR(30)     ,
  IN `P_USR_ID` VARCHAR(30)     ,
  IN `P_DPT_TYPE` VARCHAR(30)     ,
	OUT `P_RIGHT_CNT` INT             ,
	OUT `P_RESULT_ID` INT             ,
	OUT `P_MESSAGE` TINYTEXT        
)
BEGIN        
    SET P_RESULT_ID= 0;
    SET P_MESSAGE= "정상적으로 처리되었습니다.";

    IF  P_PROC_ID = 'GET_RIGHT_LIST' THEN

      SELECT COUNT(1)
        INTO P_RIGHT_CNT
        FROM SN_RIGHTGRP; 
            
      SELECT '/images/common/menu3.png' AS EDIT,
             FALSE AS SEL,
             RIG_GRP_CD,
             RIG_GRP_NM,
             RIG_GRP_DESC,
             USE_YN,
             SYS_YN,
             UPR_ID,
             UPD_TS
        FROM SN_RIGHTGRP
        ORDER BY RIG_GRP_CD ASC;
    ELSEIF P_PROC_ID = 'GET_USER_RIGHT_LIST' THEN
         
      SELECT COUNT(1)
        INTO P_RIGHT_CNT
        FROM SN_RIGHTGRP
       WHERE IFNULL(USE_YN, 'Y') != 'N';

      SELECT USR_ID, 
             RIG_GRP_CD, 
             RIG_GRP_NM, 
             RIG_GRP_DESC,             
             CASE WHEN USE_YN = 'Y' THEN true ELSE false END USE_YN,  
             UPR_ID, 
             REG_TS,  
             UPD_TS 
        FROM (  SELECT P_USR_ID AS USR_ID, 
                      B.RIG_GRP_CD,  
                      B.RIG_GRP_NM, 
                      B.RIG_GRP_DESC, 
                      IFNULL(A.USE_YN, 'N') AS USE_YN, 
                      P_USR_ID AS UPR_ID,  
                      NOW() AS REG_TS, 
                      NOW() AS UPD_TS 
                 FROM (SELECT NULL AS USR_ID,  
                              NULL AS RIG_GRP_CD, 
                              NULL AS RIG_GRP_NM, 
                              NULL AS RIG_GRP_DESC, 
                              NULL AS USE_YN,  
                              NULL AS UPR_ID) A RIGHT OUTER JOIN SN_RIGHTGRP B ON A.RIG_GRP_CD = B.RIG_GRP_CD 
                WHERE A.USR_ID IS NULL 
                  AND EXISTS (SELECT 1 FROM SN_USR WHERE USR_ID = P_USR_ID) 
                  AND B.RIG_GRP_CD NOT IN (SELECT RIG_GRP_CD FROM SN_RIGHT WHERE USR_ID = P_USR_ID) 
                  AND B.USE_YN = 'Y'       
          UNION ALL 
               SELECT USR_ID, 
                      A.RIG_GRP_CD, 
                      B.RIG_GRP_NM, 
                      B.RIG_GRP_DESC,                            
                      A.USE_YN,  
                      A.UPR_ID, 
                      A.REG_TS,  
                      A.UPD_TS 
                  FROM SN_RIGHT A, SN_RIGHTGRP B  
                 WHERE A.RIG_GRP_CD = B.RIG_GRP_CD 
                   AND USR_ID = P_USR_ID) RIG 
       ORDER BY 1, 2 ;

    ELSE
        SET P_RESULT_ID= -1;
        SET P_MESSAGE= "매개변수를 확인해주시기 바랍니다.";
    END IF;    
END ;;
