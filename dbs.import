    OUT `P_POST_NO`         		    VARCHAR(20),
    OUT `P_RESULT_ID`               INT ,	
    OUT `P_MESSAGE`                 TINYTEXT
)
BEGIN    	
    DECLARE CONTINUE HANDLER FOR SQLEXCEPTION SET P_RESULT_ID = -1;
    SET P_RESULT_ID = 0; 
    SET P_MESSAGE = "정상적으로 처리되었습니다.";
    
      SELECT IFNULL(CPNY_NM,''),
             IFNULL(CPNY_LOG_IMG_PATH,''),
             IFNULL(CPNY_ADDR,''),
             IFNULL(CEO_NM,''),
             IFNULL(BZREG_NO,''),
             IFNULL(MSTR_TEL_NO,''),
             IFNULL(MSTR_FAX_NO,''),
             IFNULL(POST_NO,'')

        INTO  P_CPNY_NM,
              P_CPNY_LOG_IMG_PATH,
              P_CPNY_ADDR,
              P_CEO_NM,
              P_BZREG_NO,
              P_MSTR_TEL_NO,
              P_MSTR_FAX_NO,
              P_POST_NO
        FROM SN_COMPANY
        WHERE CPNY_NO = P_CPNY_NO;		
    
    IF P_RESULT_ID < 0 THEN
        SET P_MESSAGE ="매개변수를 확인해주시기 바랍니다.";
    END IF;

END ;;
