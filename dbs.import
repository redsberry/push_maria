              (SELECT COUNT(USR_ID) FROM SN_USR A WHERE A.CPNY_NO = P_CPNY_NO AND A.DPT_NO = B.DPT_NO) AS DPT_USER_CNT,
              DPT_NO
      FROM SN_DPT B
      WHERE CPNY_NO = P_CPNY_NO;

    END IF;

END ;;
