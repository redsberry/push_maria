							       	)
BEGIN
    SET P_RESULT_ID	= 0;
    SET P_MESSAGE	= "정상적으로 처리되었습니다.";
	
    IF P_PROC_ID = "GET_CPNY_MEMBER" THEN
        
        SELECT  A.USR_ID        , 
                A.USR_NM        , 
                A.USR_NCKNM     ,
                A.EMAIL_ADDR    ,
                A.CELL_NO       ,
                A.TEL_NO        ,
                A.POS_NM        ,
                A.DUTY_NM       ,
                A.DPT_NO        ,
                A.AVT_IMG_PATH  ,
                GET_DEPT_NAME(A.DPT_NO) DPT_NM
          FROM  SN_USR A, V_DEVICE_USR_ID B
         WHERE  A.CPNY_NO   = P_CPNY_NO
           AND  B.USR_ID    = A.USR_ID;
		
    ELSEIF P_PROC_ID = "GET_DEPT_MEMBER" THEN
        
        SELECT  A.USR_ID        , 
                A.USR_NM        , 
                A.USR_NCKNM     ,
                A.EMAIL_ADDR    ,
                A.CELL_NO       ,
                A.TEL_NO        ,
                A.POS_NM        ,
                A.DUTY_NM       ,
                A.DPT_NO        ,
                A.AVT_IMG_PATH  ,
                GET_DEPT_NAME(A.DPT_NO) DPT_NM
          FROM  SN_USR A, V_DEVICE_USR_ID B
         WHERE  A.CPNY_NO   = P_CPNY_NO
           AND  A.DPT_NO    = P_DPT_NO
           AND  B.USR_ID    = A.USR_ID;
    
    END IF;

