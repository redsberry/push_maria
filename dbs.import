CREATE DEFINER=`redspush`@`%` PROCEDURE `GET_BOARD_MAIN`(
    IN  `P_BBS_ID` VARCHAR(5),
    OUT `P_RESULT_ID` INT,
    OUT `P_MESSAGE` TINYTEXT        
 
)
BEGIN

    SET P_RESULT_ID= 0;
    SET P_MESSAGE= "정상적으로 처리되었습니다.";
    
     SELECT BRD_CD,
             CNTS_SEQ_NO,
             CNTS_DEPTH,
             CNTS_ORD,
             CNTS_SUBJ,
             CNTS_MSG,
             ATTFILE_YN,
             REA_CNT,
             WTR_ID,
             WTR_NM,
             DATE_FORMAT(REG_TS,'%Y-%m-%d') AS REG_TS
        FROM BD_BOARD
       WHERE CNTS_DEPTH = 1 
         AND BRD_CD = P_BBS_ID
    ORDER BY CNTS_ORD ASC
    LIMIT 0, 3;
     
END ;;
